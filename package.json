{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "context-tracker",
  "title": "context-tracker",
  "description": "Track and manage your development contexts efficiently",
  "icon": "command-icon.png",
  "author": "ashwin_mirskar",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Context Tracker",
      "description": "Track your development contexts",
      "mode": "view"
    },
    {
      "name": "menubar",
      "title": "Context Menu Bar",
      "description": "Quick access to context tracking",
      "mode": "menu-bar"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
